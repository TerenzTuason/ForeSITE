{
  "version": 2,
  "buildCommand": "pip install -r requirements.txt",
  "installCommand": "python -m pip install --upgrade pip",
  "framework": null,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "PYTHON_VERSION": "3.11"
  }
}